<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.pradeep.calibration.activities.Main_screen_Activity">



    <Button
        android:id="@+id/cali_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Calibration"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/rec_button"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="@+id/rec_button"
        android:layout_marginBottom="58dp" />

    <Button
        android:id="@+id/rec_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Reconstruction"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</android.support.constraint.ConstraintLayout>
